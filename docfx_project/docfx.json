{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "CoverageKiller2/bin/Debug/CoverageKiller2.dll"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "Visibility": "Private" // ðŸ‘ˆ include internal members
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "api/**.html", "api/index.md" ]
      },
      {
        "files": [ "**.md", "toc.yml" ]
      }
    ],
    "resource": [],
    "overwrite": [],
    "template": [ "static_template/gpt" ],
    "dest": "../docs"
  }
}
